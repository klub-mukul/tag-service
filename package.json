{
  "name": "tag-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "patch-package",
    "build": "rm -rf ./dist && ./node_modules/.bin/tsc -p tsconfig.build.json",
    "start": "NODE_ENV=development node dist/main.js",
    "start:dev": "nest start --watch",
    "dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts --transpile-only",
    "watch:dev": "cross-env NODE_ENV=development nodemon --config nodemon.json",
    "debug:dev": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "prettier-format-all": "prettier --write .",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/jest --config ./test/jest-e2e.json --verbose --detectOpenHandles --forceExit --maxWorkers 2 --testTimeout 60000",
    "coverage": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --coverage --verbose --detectOpenHandles --forceExit --maxWorkers 1",
    "migration-test": "node -e \" console.log(typeof npm_config_filename); let input = 'ttt' if (!input) {process.exit(1)} \" || ts-node ./node_modules/typeorm/cli migration:generate -d ./src/config/cli-rdbms.ts ./migrations/$npm_package_config_filename",
    "migration-test-two": "ts-node -e \"if ('./migrations/' === './migrations/') {process.exit(1)} \" || ts-node ./node_modules/typeorm/cli migration:generate -d ./src/config/cli-rdbms.ts ./migrations/$npm_config_filename",
    "migration-test-three": "ts-node -e \"if (npm_config_filename === '') {process.exit(1)} \"|| pwd",
    "template-migration": "ts-node ./node_modules/typeorm/cli --f ./src/config/cli-rdbms.ts migration:create -d ./src/migrations -n",
    "add-migration": "cross-var ts-node ./node_modules/typeorm/cli migration:generate ./src/migrations/$npm_config_filename -d ./src/config/cli-rdbms.ts",
    "add-migration-backup": "ts-node ./node_modules/typeorm/cli --f ./src/config/cli-rdbms.ts migration:generate -d ./src/migrations -n",
    "migrate": "ts-node ./node_modules/typeorm/cli migration:run -d ./src/config/cli-rdbms.ts",
    "rollback": "ts-node ./node_modules/typeorm/cli --f ./src/config/cli-rdbms.ts migration:revert",
    "swagger-generator": "ts-node ./src/utils/swagger-doc-generator.ts",
    "generate-doc": "npx @compodoc/compodoc -p tsconfig.json",
    "generate-doc-prod": "npx @compodoc/compodoc -p tsconfig.json --disableSourceCode",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "prepare": "cd .. && husky install"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "csv-parse": "^5.3.10",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "fs": "^0.0.1-security",
    "install": "^0.13.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.22",
    "npm": "^9.6.6",
    "papaparse": "^5.4.1",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.15",
    "typeorm-naming-strategies": "^4.1.0",
    "url": "^0.11.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.16.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.5.0",
    "patch-package": "^7.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
